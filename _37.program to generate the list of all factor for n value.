#include <stdio.h>

// Function to print all factors of a given number n
void printFactors(int n) {
    printf("Factors of %d are: ", n);
    for (int i = 1; i <= n; i++) {
        if (n % i == 0) {
            printf("%d ", i);
        }
    }
    printf("\n");
}

int main() {
    int n;

    printf("Enter a number: ");
    scanf("%d", &n);

    // Call the function to print factors of the given number
    printFactors(n);

    return 0;
}
